{
  "name": "parcel-template-engine",
  "version": "0.1.0",
  "private": true,
  "description": "Parcel Template Engine Example",
  "author": {
    "name": "abhijithvijayan",
    "email": "email@abhijithvijayan.in",
    "url": "https://abhijithvijayan.in"
  },
  "engines": {
    "node": ">=12.18.3"
  },
  "main": "source/assets/js/index.js",
  "scripts": {
    "dev": "yarn clean && yarn parcel:serve",
    "build": "yarn clean && yarn parcel:build",
    "parcel:serve": "parcel source/*.html -p 5000",
    "parcel:watch": "parcel watch source/*.html --public-url='./'",
    "parcel:build": "parcel build source/*.html --public-url='./'",
    "clean": "rimraf dist .cache"
  },
  "dependencies": {
    "parcel": "^2.0.0-nightly.453",
    "parcel-bundler": "1.12.4"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "node-sass": "^4.14.1",
    "postcss-cli": "^7.1.2",
    "postcss-fontpath": "^1.0.0",
    "posthtml-expressions": "^1.5.0",
    "posthtml-include": "^1.5.0",
    "posthtml-load-config": "^2.0.0",
    "rimraf": "^3.0.2"
  },
  "browserslist": [
    "last 1 Chrome version"
  ]
}
